{% extends 'base.html.twig' %}

{% block title %}Agences index{% endblock %}

{% block body %}
    <h1>Agences index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Nom</th>
                <th>Ville</th>
                <th>Departement</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for agence in agences %}
            <tr>
                <td>{{ agence.id }}</td>
                <td>{{ agence.nom }}</td>
                <td>{{ agence.ville }}</td>
                <td>{{ agence.departement }}</td>
                <td>
                    <a href="{{ path('app_agences_show', {'id': agence.id}) }}">show</a>
                    <a href="{{ path('app_agences_edit', {'id': agence.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="5">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_agences_new') }}">Create new</a>
{% endblock %}
